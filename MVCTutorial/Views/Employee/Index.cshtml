@model IEnumerable<MVCTutorial.Models.Employee>
    @*notice the statement above. An IEnumerable is used to reference the Employee model.*@
@using MVCTutorial.Models;
@{
    ViewBag.Title = "Employee List";
}

<div style="font-family:Arial">
    <h2>Employee List</h2>
    <ul>
        @foreach (Employee employee in @Model)
        {
            <li>
                @Html.ActionLink(employee.Name, "Details", new { id = employee.EmployeeID })
                @*Html.ActionLink is a HTML helper method*@
            </li>
        }
    </ul>
</div>


